t');\n      return;\n    }\n    if(position === 'Right'){\n      setPosition('Middle');\n      return;\n    }\n  }\n\n  function arrowKeyRight(){\n    if(position === 'Middle'){\n      setPosition('Right');\n    }else if(position === \"Left\"){\n      setPosition('Middle');\n    }\n  }\n  useEffect(()=> {\n    window.addEventListener('keydown', positionCar);\n    return () => {\n      window.removeEventListener('keydown', positionCar);\n    }\n  },[position, gamePause, gameClass])\n  return(\n    <>\n      <Pause pauseClass={ gameClass } />\n      <Background />\n      <Laps pause={ gamePause } />\n      <CarBlue position={position} fast={velocity} />\n      <CarRed />\n    </>\n  );\n}","/home/danillo/Documentos/Projetos/DesafioFront/game-car/src/pages/Home/index.js",[],"/home/danillo/Documentos/Projetos/DesafioFront/game-car/src/pages/Home/style.js",[],"/home/danillo/Documentos/Projetos/DesafioFront/game-car/src/style/fonts.js",[],"/home/danillo/Documentos/Projetos/DesafioFront/game-car/src/components/Laps/index.js",[],"/home/danillo/Documentos/Projetos/DesafioFront/game-car/src/components/Laps/styled.js",[],"/home/danillo/Documentos/Projetos/DesafioFront/game-car/src/components/Pause/index.js",[],["97","98"],"/home/danillo/Documentos/Projetos/DesafioFront/game-car/src/components/Pause/styled.js",[],"/home/danillo/Documentos/Projetos/DesafioFront/game-car/src/pages/GameOver/index.js",[],"/home/danillo/Documentos/Projetos/DesafioFront/game-car/src/pages/GameOver/style.js",[],"/home/danillo/Documentos/Projetos/DesafioFront/game-car/src/components/CarRed/index.js",[],"/home/danillo/Documentos/Projetos/DesafioFront/game-car/src/components/CarRed/style.js",[],"/home/danillo/Documentos/Projetos/DesafioFront/game-car/src/index.js",[],"/home/danillo/Documentos/Projetos/DesafioFront/game-car/src/routes.js",[],{"ruleId":"99","replacedBy":"100"},{"ruleId":"101","replacedBy":"102"},{"ruleId":"99","replacedBy":"103"},{"ruleId":"101","replacedBy":"104"},{"ruleId":"99","replaced